{
    "version": 2,
    "builds": [{
        "src": "./index.js",
        "use": "@vercel/node"
    }, {
        "src": "./public/**",
        "use": "@vercel/static"
    }],
    "headers": [{
        "src": "./index.js",
        "headers": [{
            "key": "Cache-Control",
            "value": "s-maxage=1,stale-while-revalidate=20"

        }]
    }],
    "routes": [{
        "src": "/(.*)",
        "dest": "index.js"
    }, {
        "src": "/",
        "dest": "public"
    }, {
        "src": "/(.*)",
        "dest": "public/$1"
    }]

}