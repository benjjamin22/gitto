{
    "version": 2,
    "builds": [{
        "src": "./index.js",
        "use": "@vercel/node"
    }, {
        "src": "./mypublic/**",
        "use": "@vercel/static"
    }],
    "routes": [{
        "src": "/(.*)",
        "dest": "index.js"
    }, {
        "src": "/",
        "dest": "mypublic"
    }, {
        "src": "/(.*)",
        "dest": "mypublic/$1"
    }]

}