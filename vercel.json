{
    "version": 2,
    "name": "templates",
    "builds": [{
        "src": "./index.js",
        "use": "@vercel/node"
    }, {
        "src": "./public",
        "use": "@vercel/static"
    }],

    "routes": [{
        "src": "/(.*)",
        "dest": "index.js"
    }, {
        "src": "/",
        "dest": "public"
    }, {
        "src": "/(.*)",
        "dest": "public/$1"
    }]

}
